	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- Housing Benefit Mapper Definition -->
	<bean id="typeYHousingBenefitCheckRuleListForType_019"
