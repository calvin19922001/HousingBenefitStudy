					<property name="ruleId" value="VET-BNFT-Y-0003" />
				</bean>
				<bean
					class="hk.gov.housingauthority.nhs.housingbenefit.check.rules.BenefitCheckRulesForTypeY.WhiteFormSpouseOfTPSOwnerCheckRule">
