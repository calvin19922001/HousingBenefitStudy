			<artifactId>poi-ooxml-schemas</artifactId>
			<version>${version.poi}</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/local-lib/org.apache.poi-poi-ooxml-schemas-${version.poi}.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-compress</artifactId>
			<version>1.20</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/local-lib/org.apache.commons-commons-compress-1.20.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>org.apache.xmlbeans</groupId>
			<artifactId>xmlbeans</artifactId>
			<version>3.1.0</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/local-lib/org.apache.xmlbeans-xmlbeans-3.1.0.jar</systemPath>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>${version.compiler.plugin}</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>