			<systemPath>${project.basedir}/local-lib/org.apache.poi-poi-${version.poi}.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId>
			<version>${version.poi}</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/local-lib/org.apache.poi-poi-ooxml-${version.poi}.jar</systemPath>
